FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

epsInlet          5.19030709618504;

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform $epsInlet;

boundaryField
{
    inlet
    {
		type            turbulentMixingLengthDissipationRateInlet;
		mixingLength    0.0175;
		value			uniform 0;
	}

    outlet
    {
        type            zeroGradient;
    }

    walls
    {
        type            fixedValue;
        value           uniform 1e-10;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}

